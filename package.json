{
  "name": "norch",
  "version": "1.0.0",
  "description": "A search engine based on Node.js and LevelDB",
  "main": "./lib/norch",
  "dependencies": {
    "commander": "^7.1.0",
    "level-out": "^1.0.1",
    "mime": "^2.5.2",
    "search-index": "^3.3.0",
    "swagger-ui-dist": "^4.15.5"
  },
  "devDependencies": {
    "standard": "^17.0.0",
    "swagger-jsdoc": "^6.2.5",
    "tape": "^5.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fergiemcdowall/norch.git"
  },
  "keywords": [
    "elasticsearch",
    "search",
    "server",
    "solr",
    "free text",
    "big data"
  ],
  "bin": {
    "norch": "./bin/norch"
  },
  "maintainers": [
    "fergiemcdowall <fergusmcdowall@gmail.com>"
  ],
  "engines": {
    "node": ">18"
  },
  "scripts": {
    "build": "npm run build-openapi-docs",
    "build-openapi-docs": "rm -rf www_root/openapi && cp -R node_modules/swagger-ui-dist www_root/openapi && node src/buildSwaggerDoc.js && sed -i -e 's/https:\\/\\/petstore\\.swagger\\.io\\/v2//' www_root/openapi/swagger-initializer.js",
    "test": "rm -rf tmp && date && tape test/tape/* && standard --fix src",
    "test-cli": "./bin/norch -p 9090 -i test/sandbox/norch-cli & sleep 5s && bats test/test.bats && kill $!",
    "start": "node src/main.js"
  },
  "license": "MIT"
}
